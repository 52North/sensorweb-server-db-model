<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.n52.series.db.beans">
    <class name="PlatformEntity" table="`procedure`">
        <id name="pkid" type="long">
            <column name="pkid" />
            <generator class="assigned" />
        </id>
        <!-- <many-to-one name="service" class="ServiceEntity" foreign-key="platformServiceFk"> -->
        <!-- <column name="service" not-null="true"/> -->
        <!-- </many-to-one> -->
        <property column="domainId" insert="false" name="domainId" type="string" update="false" />
        <property column="name" name="name" type="string" />
        <property column="mobile" insert="false" name="mobile" type="org.hibernate.type.TrueFalseType" update="false" />
        <property column="insitu" insert="false" name="insitu" type="org.hibernate.type.TrueFalseType" update="false" />
    </class>
</hibernate-mapping>
