<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.n52.series.db.beans.ereporting">
    <class name="EReportingQualityEntity" table="ereporting_quality">
        <comment>Table to store the AQD e-Reporting quality information for yearly reporting. Mapping file: mapping/ereporting/EReportingQuality.hbm.xml</comment>
        <id name="id" type="long">
            <column name="id">
                <comment>Table primary key</comment>
            </column>
            <generator class="native">
                <param name="sequence_name">ereporting_quality_seq</param>
            </generator>
        </id>
        <property name="dataset" type="long">
               <column name="dataset" not-null="true" unique-key="ereporting_quality_id">
                   <comment>Relation/foreign key to the associated series table. Contains "dataset".id</comment>
               </column>
        </property>
        <property name="year" type="int">
               <column name="year" not-null="true" unique-key="ereporting_quality_id">
                   <comment>referenced year</comment>
               </column>
        </property>
        <property name="primaryObservation" type="string">
            <column name="primary_observation" not-null="true" unique-key="ereporting_quality_id">
                   <comment>primaryObservation flag, see http://dd.eionet.europa.eu/vocabulary/aq/primaryObservation/view</comment>
               </column>
        </property>
        <property name="timeCoverageFlag" type="org.hibernate.type.NumericBooleanType">
            <column name="time_coverage_flag" check="time_coverage_flag in (1,0)">
                   <comment>indicator for time coverage </comment>
               </column>
        </property>
        <property name="dataCaptureFlag" type="org.hibernate.type.NumericBooleanType">
            <column name="data_capture_flag" check="data_capture_flag in (1,0)">
                   <comment>Data capture value, used for multi day aggregations</comment>
               </column>
        </property>
        <property name="uncertaintyEstimation" type="double">
            <column name="uncertainty_estimation" not-null="false">
                   <comment>value for uncertainty estimation</comment>
               </column>
        </property>
    </class>
</hibernate-mapping>
