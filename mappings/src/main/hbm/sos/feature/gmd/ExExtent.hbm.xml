<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.n52.series.db.beans.feature.gmd">
    <class name="ExExtentEntity"
           table="exextent">
        <id name="pkid"
            type="long">
            <column name="pkid">
                <comment>Table primary key, used for relations</comment>
            </column>
            <generator class="native">
                <param name="sequence">exExtent_seq</param>
            </generator>
        </id>
        <property name="id" column="id" type="string" />
        <property name="uuid" column="uuid" type="string" />
        <property name="description" column="description" type="string" />
        <set name="verticalExtent" table="exextentverticalext">
          <key column="exExtent" not-null="true" foreign-key="exextentextentfk"/>
          <many-to-many column="exVerticalExtent"
                        class="ExVerticalExtentEntity" foreign-key="exextentverticalfk"/>
        </set>
    </class>
</hibernate-mapping>
