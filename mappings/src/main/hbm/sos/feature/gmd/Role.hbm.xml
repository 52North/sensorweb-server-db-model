<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.n52.series.db.beans.feature.gmd">
    <class name="RoleEntity"
           table="role">
        <id name="id"
            type="long">
            <column name="id">
                <comment>Table primary key, used for relations</comment>
            </column>
            <generator class="native">
                <param name="sequence">role_seq</param>
            </generator>
        </id>
        <property name="codeList" column="codeList" not-null="true" type="string" />
        <property name="codeListValue" column="codeListValue" not-null="true" type="string" />
    </class>
</hibernate-mapping>
