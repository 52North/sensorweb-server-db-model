<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping package="org.n52.series.db.beans.sta"
                   xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping http://www.hibernate.org/xsd/hibernate-mapping/hibernate-mapping-4.0.xsd">
    <class name="DatastreamEntity" table="datastream">
        <id name="id" type="long">
            <column name="location_id"/>
            <generator class="native">
                <param name="sequence_name">offering_seq</param>
            </generator>
        </id>
        <property name="name" type="string">
            <column name="name" not-null="true"/>
        </property>
        <property name="description" type="string">
            <column name="description" not-null="true"/>
        </property>
    </many-to-one>
    <component class="GeometryEntity" name="geometryEntity">
        <property name="geometry">
            <column name="observedArea" type="jts_geometry" not-null="false"/>
        </property>
    </component>
    <property name="resultTimeStart" type="timestamp">
        <column name="resultTimeStart" length="29" not-null="false" default="NULL"/>
    </property>
    <property name="resultTimeEnd" type="timestamp">
        <column name="resultTimeEnd" length="29" not-null="false" default="NULL"/>
    </property>
    <property name="samplingTimeStart" type="timestamp">
        <column name="phenomenonTimeStart" length="29" not-null="false" default="NULL"/>
    </property>
    <property name="samplingTimeEnd" type="timestamp">
        <column name="phenomenonTimeEnd" length="29" not-null="false" default="NULL"/>
    </property>
    <many-to-one name="observationType" class="FormatEntity" not-null="false" foreign-key="fk_datastream_observation_type">
        <column name="fk_format_id" not-null="true"/>
    </many-to-one>
    <many-to-one name="unitOfMeasurement" class="FormatEntity" not-null="false" foreign-key="fk_datastream_unit">
        <column name="fk_unit_id" not-null="true"/>
    </many-to-one>
    <many-to-one name="thing" class="ThingEntity" not-null="true" foreign-key="fk_datastream_thing">
        <column name="fk_thing_id" not-null="true"/>
    </many-to-one>
    <many-to-one name="procedure" class="ProcedureEntity" not-null="true" foreign-key="fk_datastream_procedure">
        <column name="fk_procedure_id" not-null="true"/>
    </many-to-one>
    <many-to-one name="phenomenen" class="PheonemenonEntity" not-null="true" foreign-key="fk_datastream_phenomenon">
        <column name="fk_phenomenon_id" not-null="true"/>
    </many-to-one>
        
    <set name="datasets" table="datastream_dataset" inverse="false">
        <key foreign-key="fk_datastream_dataset">
            <column name="fk_datastream_id" not-null="true"/>
        </key>
        <many-to-many class="DatasetEntity" foreign-key="fk_dataset_datastream">
            <column name="fk_dataset_id" not-null="true"/>
        </many-to-many>
    </set>
    </class>

</hibernate-mapping>
