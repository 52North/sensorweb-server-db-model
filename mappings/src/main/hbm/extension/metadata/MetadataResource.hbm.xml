<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping package="org.n52.series.db.beans.metadata"
	xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping http://www.hibernate.org/xsd/hibernate-mapping/hibernate-mapping-4.0.xsd">
    <class name="MetadataEntity" table="dataset_metadata">
    	<composite-id>
    		<key-property name="dataset_id" column="fk_dataset_id" type="long"/>
    		<key-many-to-one name="parameter"  class="org.n52.series.db.beans.parameter.Parameter" column="fk_parameter_id"
                foreign-key="fk_parameter_metadtata" />
    	</composite-id>
        <property name="lastUpdate" column="last_update" type="timestamp" />
      	<property name="domain" column="domain" type="string" />
    </class>
</hibernate-mapping>
